{
  "name": "Spoiler Blocker",
  "description": "Filters out spoilers for movies and TV series of your choice",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the menu"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}